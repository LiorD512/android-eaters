on:
  push:
    # Sequence of patterns matched against refs/tags
    tags:
      - 'prod-v*' # Push events to matching v*, i.e. v1.0, v20.15.10

name: send to qa (prod) and notify slack
jobs:
  build:

    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v1
      - uses: ./.github/workflows/send-to-qa/
      - uses: ./.github/workflows/slack-notifier/