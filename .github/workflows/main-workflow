on:
  push:
    # Sequence of patterns matched against refs/tags
    tags:
      - 'prod-v*' # Push events to matching v*, i.e. v1.0, v20.15.10

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v1
      - uses: ./send-to-qa/send-to-qa-production.yml
      - uses: ./slack-notifier/slack-notify.yml